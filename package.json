  "scripts": {
    "start": "webpack serve --env mode=development",
    "start:vite": "vite",
    "start:esbuild": "ts-node ./configs/esbuild/esbuild-dev.ts",
    "start:dev": "concurrently \"pnpm start\" \"pnpm run start:dev:server\"",
    "start:dev:vite": "concurrently \"pnpm run start:vite\" \"pnpm run start:dev:server\"",
    "start:dev:esbuild": "concurrently \"pnpm run start:esbuild\" \"pnpm run start:dev:server\"",
    "start:debug": "webpack serve --env mode=development",
    "start:dev:server": "node ./json-server/index.js",
    "build:dev": "webpack --env mode=development",
    "build:dev:esbuild": "cross-env MODE=development ts-node ./configs/esbuild/esbuild-dev.ts",
    "build:prod": "webpack --env mode=production",
    "build:prod:vite": "vite build",
    "build:prod:esbuild": "cross-env MODE=production ts-node ./configs/esbuild/esbuild-prod.ts",
    "lint:ts": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
    "prettier": "prettier --write \"**/*.{ts,tsx,json}\"",
    "lint:scss": "stylelint \"**/*.{scss,sass,css}\"",
    "lint:scss:fix": "stylelint \"**/*.{scss,sass,css}\" --fix",
    "test:unit": "jest --config ./configs/webpack/jest/jest.config.ts",
    "test:unit:report": "jest --config ./configs/webpack/jest/jest.config.ts --json --outputFile=./reports/unit/.jest-test-results.json",
    "test:e2e": "cypress open",
    "test:ui": "loki test",
    "test:ui:ok": "loki approve",
    "test:ui:ci": "loki update --requireReference --reactUri file:./storybook-static",
    "test:ui:json": "node ./scripts/generate-visual-json-report.js",
    "test:ui:html": "npx reg-cli --from .loki/report.json --report .loki/report.html",
    "test:ui:report": "pnpm run test:ui:json && pnpm run test:ui:html",
    "test:depend": "npx depcruise src --include-only '^src' --config --output-type dot | dot -T svg > reports/dependency-graph/dependency-graph.svg",
    "storybook": "storybook dev -p 6006 -c ./configs/webpack/storybook",
    "storybook:debug": "storybook dev -p 6006 -c ./configs/webpack/storybook --debug-webpack",
    "storybook:test": "test-storybook -c ./configs/webpack/storybook",
    "build-storybook": "storybook build -c ./configs/webpack/storybook",
    "chromatic": "chromatic --exit-zero-on-changes --project-token=430a91684282",
    "prepare": "husky install",
    "generate:slices": "node ./scripts/createSlice/index.js",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "node ./scripts/remove-cache.js",
    "remove-features": "npx ts-node ./scripts/remove-feature.ts"
  },
